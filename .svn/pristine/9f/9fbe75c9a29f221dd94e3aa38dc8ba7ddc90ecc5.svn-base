package kr.co.jtimes.news.comment.vo;

import java.util.Date;
import kr.co.jtimes.news.vo.TbNewsVo;
import kr.co.jtimes.profile.common.vo.UserVo;

public class NewCommentVo {

	private int no;
	private Date regDate;
	private TbNewsVo news;
	private UserVo user;
	
	public NewCommentVo() {}

	public NewCommentVo(int no, Date regDate, TbNewsVo news, UserVo user) {
		super();
		this.no = no;
		this.regDate = regDate;
		this.news = news;
		this.user = user;
	}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public Date getRegDate() {
		return regDate;
	}

	public void setRegDate(Date regDate) {
		this.regDate = regDate;
	}

	public TbNewsVo getNews() {
		return news;
	}

	public void setNews(TbNewsVo news) {
		this.news = news;
	}

	public UserVo getUser() {
		return user;
	}

	public void setUser(UserVo user) {
		this.user = user;
	}

	@Override
	public String toString() {
		return "CommentVo [no=" + no + ", regDate=" + regDate + ", news=" + news + ", user=" + user + "]";
	}
	
	
}
