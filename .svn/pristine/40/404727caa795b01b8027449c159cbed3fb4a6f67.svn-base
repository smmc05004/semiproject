package kr.co.jtimes.reporter.imagecontainer.common.vo;

import kr.co.jtimes.common.vo.NewsCategoryVo;
import kr.co.jtimes.common.vo.NewsTypeVo;
import kr.co.jtimes.reporter.profile.common.vo.ReporterEmployeeVo;

public class TbImageVo {

	private int imgNo;
	private String imgLocation;
	private String imgTitle;
	private String imgAlt;
	private String imgPath;
	private String imgAlterState;
	private ReporterEmployeeVo reporterNo;
	private NewsTypeVo newsTypeNo;
	private NewsCategoryVo categoryNo;
	private String imgType;
	private String imgSize;
	
	public TbImageVo() {}

	public TbImageVo(int imgNo, String imgLocation, String imgTitle, String imgAlt, String imgPath,
			String imgAlterState, ReporterEmployeeVo reporterNo, NewsTypeVo newsTypeNo, NewsCategoryVo categoryNo,
			String imgType, String imgSize) {
		super();
		this.imgNo = imgNo;
		this.imgLocation = imgLocation;
		this.imgTitle = imgTitle;
		this.imgAlt = imgAlt;
		this.imgPath = imgPath;
		this.imgAlterState = imgAlterState;
		this.reporterNo = reporterNo;
		this.newsTypeNo = newsTypeNo;
		this.categoryNo = categoryNo;
		this.imgType = imgType;
		this.imgSize = imgSize;
	}

	public int getImgNo() {
		return imgNo;
	}

	public void setImgNo(int imgNo) {
		this.imgNo = imgNo;
	}

	public String getImgLocation() {
		return imgLocation;
	}

	public void setImgLocation(String imgLocation) {
		this.imgLocation = imgLocation;
	}

	public String getImgTitle() {
		return imgTitle;
	}

	public void setImgTitle(String imgTitle) {
		this.imgTitle = imgTitle;
	}

	public String getImgAlt() {
		return imgAlt;
	}

	public void setImgAlt(String imgAlt) {
		this.imgAlt = imgAlt;
	}

	public String getImgPath() {
		return imgPath;
	}

	public void setImgPath(String imgPath) {
		this.imgPath = imgPath;
	}

	public String getImgAlterState() {
		return imgAlterState;
	}

	public void setImgAlterState(String imgAlterState) {
		this.imgAlterState = imgAlterState;
	}

	public ReporterEmployeeVo getReporterNo() {
		return reporterNo;
	}

	public void setReporterNo(ReporterEmployeeVo reporterNo) {
		this.reporterNo = reporterNo;
	}

	public NewsTypeVo getNewsTypeNo() {
		return newsTypeNo;
	}

	public void setNewsTypeNo(NewsTypeVo newsTypeNo) {
		this.newsTypeNo = newsTypeNo;
	}

	public NewsCategoryVo getCategoryNo() {
		return categoryNo;
	}

	public void setCategoryNo(NewsCategoryVo categoryNo) {
		this.categoryNo = categoryNo;
	}

	public String getImgType() {
		return imgType;
	}

	public void setImgType(String imgType) {
		this.imgType = imgType;
	}

	public String getImgSize() {
		return imgSize;
	}

	public void setImgSize(String imgSize) {
		this.imgSize = imgSize;
	}

	
	
	
}
